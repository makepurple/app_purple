{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**",
				"storybook-static",
				".next/**"
			]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"@makepurple/prisma#build": {
			"cache": false
		},
		"@makepurple/www#build": {
			"dependsOn": [
				"^build",
				"@makepurple/prisma#build"
			],
			"outputs": [
				".next/**"
			]
		}
	}
}
